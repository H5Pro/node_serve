{
  "name": "node_serve",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "npm run build && NODE_ENV=production ts-node ./server/index.ts",
    "dev": "NODE_ENV=development node -r ts-node/register --inspect ./server/index.ts",
    "watch": "nodemon --watch './server/*.ts' -e ts,tsx --exec 'npm run dev'"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.3",
    "element-ui": "^2.13.0",
    "express": "^4.17.1",
    "http": "0.0.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^0.20.0",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-proxy-middleware": "0.0.4",
    "lodash": "^4.17.15",
    "request": "^2.88.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/http-proxy": "^1.17.2",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/koa": "^2.0.52",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/lodash": "^4.14.149",
    "@types/request": "^2.48.4",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "ts-node": "^8.5.2",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
